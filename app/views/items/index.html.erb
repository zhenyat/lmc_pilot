<h1>Listing items</h1>

<table>
  <thead>
    <tr>
      <th>Assessment</th>
      <th>Behavior</th>
      <th>User tick</th>
      <th>Final tick</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @items.each do |item| %>
      <tr>
        <td><%= item.assessment %></td>
        <td><%= item.behavior %></td>
        <td><%= item.user_tick %></td>
        <td><%= item.final_tick %></td>
        <td><%= link_to 'Show', item %></td>
        <td><%= link_to 'Edit', edit_item_path(item) %></td>
        <td><%= link_to 'Destroy', item, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Item', new_item_path %>
